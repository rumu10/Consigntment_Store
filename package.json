{
  "name": "Consigntment Store",
  "version": "0.1.0",
  "private": true,
  "author": [
    "Jaime Bowen",
    "Sudipta Biswas",
    "Feiquan Liu",
    "Dong Tang"
  ],
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install": "concurrently  \"npm run install:backend\" \"npm run install:frontend\"",
    "deploy:backend": "cd backend && npm run deploy",
    "deploy:frontend": "cd frontend && npm run deploy",
    "deploy": "concurrently \"npm run deploy:backend\" \"npm run deploy:frontend\""
  },

  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
